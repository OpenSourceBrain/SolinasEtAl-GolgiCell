# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation 

target: ../Test_Soma.hoc
engine: NEURON

mep: .test.soma.mep
experiments:
  pacemaking:
    observables:
      spike times:
        file: 
          path: ../Soma.si.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0

